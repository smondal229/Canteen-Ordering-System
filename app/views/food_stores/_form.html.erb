<%= form_for(@food_store) do |f| %>
  <%= render(partial: "shared/error_messages", locals: { target: @food_store }) %>

  <div class = "form-group">
    <%= f.label :name %>
    <%= f.text_field(:name, class: "form-control", placeholder: "Enter Food Store name") %>
  </div>

  <div class = "form-group">
    <%= f.submit "Submit", class: "m-2" %>
  </div>
<% end %>
